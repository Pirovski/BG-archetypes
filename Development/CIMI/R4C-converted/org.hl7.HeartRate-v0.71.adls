            at0013, -- Normal
            at0014, -- Strong
            at0015] -- Weak
          }
        }
        ELEMENT[at0016] occurrences matches {1..1} matches { -- Location
          value matches {
            [local::
            at0017, -- Arteria brachialis
            at0018, -- Arteria carotis
            at0019, -- Arteria carotis externa
            at0020, -- Arteria dorsalis pedis
            at0021, -- Arteria femoralis
