            at0026, -- Arteria tibialis
            at0027, -- Arteria tibialis posterior
            at0028] -- Fontanel (baby)
          }
        }
        ELEMENT[at0029] occurrences matches {1..1} matches { -- Volume
          value matches {
            [local::
            at0030, -- Large
            at0031, -- Normal
            at0032] -- Weak
          }
        }
        ELEMENT[at0033] occurrences matches {0..1} matches { -- PointInTime
          value matches {
            [local::
            at0034, -- 1H min
            at0035, -- 1H max
            at0036] -- 1H mean
          }
        }
        ELEMENT[at0037] occurrences matches {1..1} matches { -- FrequencyQualification
          value matches {
            [local::
            at0038, -- Alternating bradycardia and tachycardia
            at0039, -- Bradycardia
            at0040, -- Normal
            at0041] -- Tachycardia
          }
        }
        ELEMENT[at0042] occurrences matches {1..1} matches { -- Regularity
          value matches {
            [local::
            at0043, -- Irregular
            at0044] -- Regular
          }
        }
        ELEMENT[at0045] occurrences matches {1..1} matches { -- BodyPosition
          value matches {
            [local::
            at0046, -- Lying
            at0047, -- Sitting
            at0048, -- Standing
            at0049] -- Reclining
          }
        }
        ELEMENT[at0050] occurrences matches {1..1} matches { -- Exertion
          value matches {
            [local::
            at0051, -- In rest
            at0052, -- Post exercise
            at0053] -- During exercise
          }
        }
        ELEMENT[at0054] occurrences matches {1..1} matches { -- Method
          value matches {
            [local::
            at0055, -- Auscultation
            at0056, -- BedsideMonitor
            at0057, -- ECG
            at0058, -- Palpation/ manual
            at0059] -- Ultrasound
          }
        }
    }
  }

