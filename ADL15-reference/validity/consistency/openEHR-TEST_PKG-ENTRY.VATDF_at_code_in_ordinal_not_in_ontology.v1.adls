archetype (adl_version=1.5)
	openEHR-TEST_PKG-ENTRY.VATDF_at_code_in_ordinal_not_in_terminology.v1

language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"Peter Gummer">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Test VATDF validity check, triggered by an at-code used in an ordinal is not in the terminology.">
			keywords = <"ADL", "validation", "test">
			copyright = <"copyright (c) 2008-2012 The openEHR Foundation">
		>
	>
	other_details = <
		["regression"] = <"VATDF">
	>
	lifecycle_state = <"draft">

definition
	ENTRY[at0000] matches {
		ordinal_attr_1 matches {
			DV_ORDINAL matches {
				[value, symbol] matches {
					[{0}, {[local::at0001]}], 	-- +
					[{1}, {[local::at0002]}], 	-- ++
					[{2}, {[local::at0003]}] 	-- missing
				}
			}
		}
	}

