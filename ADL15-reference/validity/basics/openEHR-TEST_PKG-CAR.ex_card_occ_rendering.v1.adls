archetype (adl_version=1.5)
	openEHR-TEST_PKG-CAR.ex_card_occ_rendering.v1

language
	original_language = <[ISO_639-1::en]>

description
	original_author = <
		["name"] = <"Thomas Beale">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Test visual rendering of existence, cardinality and occurrences in the ADL Workbench.">
			copyright = <"copyright (c) 2006 The openEHR Foundation">
		>
	>
	other_details = <
		["regression"] = <"PASS">
	>
	lifecycle_state = <"draft">

definition
	CAR[at0000] matches {	-- car
		body matches {
			CAR_BODY[at0001] matches {
				 model existence matches {1} matches {"aston martin"}
			}
		}
		engine_parts existence matches {1} matches {
			ENGINE_PART[at0002] matches {	
				items existence matches {1} cardinality matches {0..*} matches {
					ENGINE_PART_ITEM[at0012]
				}
			}
			ENGINE_PART[at0003] matches {
				items cardinality matches {1..*} matches {
					ENGINE_PART_ITEM[at0004]
					ENGINE_PART_ITEM[at0005] occurrences matches {0..1}
					ENGINE_PART_ITEM[at0006] occurrences matches {1}
					ENGINE_PART_ITEM[at0007] occurrences matches {6..12} 
					ENGINE_PART_ITEM[at0008] occurrences matches {12}
					ENGINE_PART_ITEM[at0009] occurrences matches {4}
					ENGINE_PART_ITEM[at0010] occurrences matches {1..5}
					ENGINE_PART_ITEM[at0011] occurrences matches {10..20}
				}
			}
			ENGINE_PART[at0012] matches {	
				items cardinality matches {2..5} matches {
					ENGINE_PART_ITEM[at0013]
				}
			}
		}
	}

