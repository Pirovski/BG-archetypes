		["organisation"] = <"Ocean Informatics">
		["email"] = <"thomas.beale@oceaninformatics.com">
		["date"] = <"14/03/2010">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Test VDSEV validity check triggered by both include and exclude set to 'any' constraint">
		>
	>
	other_details = <
		["regression"] = <"VDSEV">
	>
