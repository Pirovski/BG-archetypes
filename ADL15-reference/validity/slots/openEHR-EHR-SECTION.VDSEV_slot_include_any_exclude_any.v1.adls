definition
	SECTION[at0000] matches {	
		items cardinality matches {1..*; unordered} matches {
			allow_archetype OBSERVATION[at0001] occurrences matches {0..1} matches {	
				include
					archetype_id/value matches {/.*/}
				exclude
					archetype_id/value matches {/.*/}
			}
		}
	}

