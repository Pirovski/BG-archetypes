	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record details about a single anatomical location, including the ability to record an approximate location relative to identified anatomical landmarks.">
			use = <"Use within slots in ENTRY and CLUSTER archetypes to describe anatomical location in a consistent manner.

Examples of use: 
- Use within CLUSTER.specimen to record Anatomical Location details within the 'Collection Site' slot.
- Use within EVALUATION.adverse_reaction to record Anatomical Location details within the 'Additional Reaction Detail' slot.">
			keywords = <"location", "site", "anatomical", "structure", "area">
			misuse = <"Not for specifiying unilateral/bilateral occurrence of a health condition or diagnosis - this is an evaluative statement.">
			copyright = <"Â© openEHR">
		>
	>
	lifecycle_state = <"unmanaged">
	other_details = <
		["references"] = <"Use cases:

- 5LICS, midclavicular line

- L forearm, 2cm anterior to the cubital fossa

- Right iliac fossa

- 5 cm below L tibial tuberosity

- 2 cm medial to R nipple

- 4 cm below R costal margin in midclavicular line

- R femur

- midline nose

- medial aspect of R great toe nail.">
		["MD5-CAM-1.0.1"] = <"ABE2EBFBA88F794273B956613748E3E5">
	>

definition
	CLUSTER[id1] matches {	-- Anatomical Location
		items matches {
			ELEMENT[id2] occurrences matches {0..1} matches {	-- Name
				value matches {
					DV_TEXT[id55] 
				}
			}
			ELEMENT[id24] matches {	-- Description
				value matches {
					DV_TEXT[id56] 
				}
			}
			ELEMENT[id3] occurrences matches {0..1} matches {	-- Side
				value matches {
					DV_CODED_TEXT[id57] matches {
						defining_code matches {[ac1]}		-- Side (synthesised)
					}
				}
			}
			ELEMENT[id29] occurrences matches {0..1} matches {	-- Numerical identifier
				value matches {
					DV_CODED_TEXT[id58] matches {
						defining_code matches {[ac2]}		-- Numerical identifier (synthesised)
					}
				}
			}
			ELEMENT[id25] occurrences matches {0..1} matches {	-- Anatomical Plane
				value matches {
					DV_CODED_TEXT[id59] matches {
						defining_code matches {[ac3]}		-- Anatomical Plane (synthesised)
					}
				}
			}
			CLUSTER[id21] matches {	-- Relative location
				items matches {
					ELEMENT[id22] occurrences matches {0..1} matches {	-- Identified Landmark
						value matches {
							DV_TEXT[id60] 
						}
					}
					ELEMENT[id7] occurrences matches {0..1} matches {	-- Aspect
						value matches {
							DV_CODED_TEXT[id61] matches {
								defining_code matches {[ac4]}		-- Aspect (synthesised)
							}
						}
					}
					ELEMENT[id23] occurrences matches {0..1} matches {	-- Distance from Landmark
						value matches {
							DV_QUANTITY[id62] matches {
								property matches {[at53]}
								units matches {"cm", "in", "mm"}
							}
						}
					}
				}
			}
			ELEMENT[id54] matches {	-- Visual Markings/Orientation
				value matches {
					DV_TEXT[id63] 
				}
			}
			ELEMENT[id53] matches {	-- Image
				value matches {
					DV_MULTIMEDIA[id64] matches {
						media_type 
					}
				}
			}
		}
	}

