definition
	OBSERVATION[at0000.1] matches {	-- Lipid Studies Test Result
		/data[at0001]/events[at0002]/data[at0003]/items[at0095]/items[at0096.6]/items[at0078.1]/value matches {
			DV_QUANTITY[id0.613] matches {
				property matches {[openehr::119]}
				units matches {"mg/dl", "mmol/l"}
			}
		}
		/data[at0001]/events[at0002]/data[at0003]/items[at0095]/items[at0096.7]/items[at0078.2]/value matches {
			DV_QUANTITY[id0.653] matches {
				property matches {[openehr::119]}
				[magnitude, units] matches {
					[{|>=0.0|}, {"mg/dl"}],
					[{|>=0.0|}, {"mmol/l"}]
				}
			}
		}
		/data[at0001]/events[at0002]/data[at0003]/items[at0095]/items[at0096.5]/items[at0078.3]/value matches {
