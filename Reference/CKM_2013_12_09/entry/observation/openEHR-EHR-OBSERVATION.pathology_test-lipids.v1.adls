		/data[at0001]/events[at0002]/data[at0003]/items[at0095]/items[at0096.3]/items[at0078.8]/value matches {
			DV_QUANTITY[id0.109] matches {
				property matches {[openehr::119]}
				[magnitude, units] matches {
					[{|>=0.0|}, {"mg/dl"}],
					[{|>=0.0|}, {"mmol/l"}]
				}
			}
		}
		/data[at0001]/events[at0002]/data[at0003]/items[at0095]/items[at0096.2]/items[at0078.5]/value matches {
			DV_PROPORTION[id0.77] matches {
				is_integral matches {False}
				type matches {0}
			}
		}
		/data[at0001]/events[at0002]/data[at0003]/items[at0095]/items[at0096.1]/items[at0078.6]/value matches {
			DV_PROPORTION[id0.645] matches {
				is_integral matches {False}
				type matches {0}
			}
		}
		/data[at0001]/events[at0002]/state matches {
			ITEM_TREE[at0.112] matches {	-- Tree
				items matches {
					ELEMENT[at0.113] occurrences matches {0..1} matches {	-- Fasting State
						value matches {
							DV_BOOLEAN[id0.229] matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0.114] occurrences matches {0..1} matches {	-- Duration of Fasting
						value matches {
							DV_DURATION[id0.629] matches {
								value matches {PTHS/|>=PT0S|}
							}
						}
					}
				}
			}
		}
		/protocol[at0004]/items matches {
			before [at0017]
			ELEMENT[at0.111] occurrences matches {0..1} matches {	-- Point of Care Device used
				value matches {
					DV_BOOLEAN[id0.621] matches {
						value matches {True, False}
					}
				}
			}
		}
	}

