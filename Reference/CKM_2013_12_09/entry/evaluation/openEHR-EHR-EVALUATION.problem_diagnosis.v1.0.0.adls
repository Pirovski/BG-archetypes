definition
	EVALUATION[id1] matches {	-- Problem/Diagnosis
		data matches {
			ITEM_TREE[id2] matches {
				items cardinality matches {1..*; ordered} matches {
					ELEMENT[id3] matches {	-- Problem/Diagnosis
						value matches {
							DV_TEXT[id48] 
						}
					}
					ELEMENT[id10] occurrences matches {0..1} matches {	-- Description
						value matches {
							DV_TEXT[id49] 
						}
					}
					ELEMENT[id6] occurrences matches {0..1} matches {	-- Severity
						value matches {
							DV_CODED_TEXT[id50] 
						}
					}
					ELEMENT[id4] occurrences matches {0..1} matches {	-- Date of Onset
						value matches {
							DV_DATE_TIME[id51] matches {
								value matches {yyyy-mm-ddTHH:MM:SS}
							}
						}
					}
					ELEMENT[id5] occurrences matches {0..1} matches {	-- Age at Onset
						value matches {
							DV_DURATION[id52] 
						}
					}
					ELEMENT[id13] occurrences matches {0..1} matches {	-- Body Site
						value matches {
							DV_TEXT[id53] 
						}
					}
					allow_archetype CLUSTER[id40] occurrences matches {0..1} matches {	-- Body Site Details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
					}
					CLUSTER[id19] occurrences matches {0..1} matches {	-- Previous Occurrences
						items matches {
							ELEMENT[id20] occurrences matches {0..1} matches {	-- Frequency
								value matches {
									DV_QUANTITY[id54] matches {
										property matches {[at43]}
										[magnitude, units] matches {
											[{|>=0.0|}, {"/min"}],
											[{|>=0.0|}, {"/h"}],
											[{|>=0.0|}, {"/d"}],
											[{|>=0.0|}, {"/wk"}],
											[{|>=0.0|}, {"/mo"}],
											[{|>=0.0|}, {"/yr"}]
										}
									}
								}
							}
							ELEMENT[id21] occurrences matches {0..1} matches {	-- Date of last Occurrence
								value matches {
									DV_DATE_TIME[id55] matches {
										value matches {yyyy-mm-ddTHH:MM:SS}
									}
								}
							}
							ELEMENT[id26] occurrences matches {0..1} matches {	-- Number
								value matches {
									DV_COUNT[id56] matches {
										magnitude matches {|>=1|}
									}
								}
							}
							ELEMENT[id23] occurrences matches {0..1} matches {	-- Description
								value matches {
									DV_TEXT[id57] 
								}
							}
						}
					}
					CLUSTER[id28] matches {	-- Related Item
						items matches {
							ELEMENT[id30] occurrences matches {0..1} matches {	-- Relationship Type
								value matches {
									DV_CODED_TEXT[id58] matches {
										defining_code matches {[ac1]}		-- Relationship Type (synthesised)
									}
								}
							}
							ELEMENT[id29] occurrences matches {0..1} matches {	-- Item
								value matches {
									DV_TEXT[id59] 
									DV_URI[id60] 
								}
							}
							ELEMENT[id45] occurrences matches {0..1} matches {	-- Description
								value matches {
									DV_TEXT[id61] 
								}
							}
						}
					}
					ELEMENT[id31] occurrences matches {0..1} matches {	-- Date of Resolution
						value matches {
							DV_DATE[id62] 
						}
					}
					ELEMENT[id32] occurrences matches {0..1} matches {	-- Age at Resolution
						value matches {
							DV_DURATION[id63] 
						}
					}
					allow_archetype CLUSTER[id43] occurrences matches {0..1} matches {	-- Diagnostic Criteria
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[id44] matches {	-- Clinical Stage/Grade
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[id33] matches {
				items matches {
					ELEMENT[id36] matches {	-- Supporting clinical evidence
						value matches {
							DV_EHR_URI[id64] 
						}
					}
					allow_archetype CLUSTER[id47] matches {	-- Status
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

